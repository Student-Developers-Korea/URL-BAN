{
  "manifest_version": 2,
  "name": "URL BANNER",
  "version": "0.0.1",
  "author": "Genius0412",
  "description": "원하시는 사이트를 밴 해드립니다",
  "permissions": [
    "tabs",
    "storage",
    "*://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": "media/logo-2.png",
    "default_title": "URL BANNER",
    "default_popup": "popup.html"
  }
}